{
  "name": "@agentvet/cli",
  "version": "0.17.0",
  "description": "Security scanner for AI agent skills, configs, and MCP tools. Vet before you trust.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agentvet": "bin/agentvet.js",
    "agentvet-manifest": "bin/manifest.js"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "dev": "node bin/agentvet.js",
    "test": "npm run build && node --test test/*.test.js test/rules/*.test.js test/firewall/*.test.js",
    "lint": "eslint src bin",
    "lint:fix": "eslint src bin --fix",
    "scan": "node bin/agentvet.js scan .",
    "scan:sarif": "node bin/agentvet.js scan . --format sarif --output results.sarif",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "security",
    "ai-agent",
    "scanner",
    "mcp",
    "skills",
    "clawdbot",
    "claude-code",
    "devin",
    "supply-chain"
  ],
  "author": "AgentVet Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taku-tez/agentvet.git"
  },
  "bugs": {
    "url": "https://github.com/taku-tez/agentvet/issues"
  },
  "homepage": "https://github.com/taku-tez/agentvet#readme",
  "devDependencies": {
    "@types/node": "^25.2.0",
    "eslint": "^9.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "yara",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "yaml": "^2.8.2"
  }
}
